{
  "manifest_version": 2,

  "name": "Würfel-Armin Erweiterung",
  "version": "0.1.4",
  "description": "Turns every mention of Armin Laschet into Würfel-Armin",
  "icons": { "16": "images/dice16.png",
           "48": "images/dice48.png",
          "128": "images/dice128.png" },
  "content_scripts": [
    {
    "matches": ["*://*/*"],
    "js": ["content.js"],
    "run_at":"document_end"

  }
]

}
